# Copy this file to .env.local and fill with your Firebase Web App config
# Values must be from Firebase Console → Project Settings → Your apps → Config

VITE_FIREBASE_API_KEY=YOUR_API_KEY
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=000000000000
VITE_FIREBASE_APP_ID=1:000000000000:web:abcdef1234567890
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Backend API base (no trailing slash). Defaults to Render deployment if unset.
VITE_API_BASE_URL=https://docai-backend-8ze0.onrender.com